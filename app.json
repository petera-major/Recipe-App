{
  "expo": {
    "name": "SmartPantryMuse",
    "slug": "smartpantrymuse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "recipe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.peteram.smartpantrymuse",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "We need access to your camera to let you snap photos of your finished dishes.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to let you share your food photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "SmartPantryMuse",
      "shortName": "PantryMuse",
      "themeColor": "#fdf5ec",
      "backgroundColor": "#fff",
      "display": "standalone"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-sharing",
      "expo-file-system",
      [
        "expo-splash-screen",
        {
          "resizeMode": "cover",
          "backgroundColor": "#fff5ec"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e42892b-89d3-409d-b3c0-d01671350991"
      }
    },
    "owner": "peteram"
  }
}
